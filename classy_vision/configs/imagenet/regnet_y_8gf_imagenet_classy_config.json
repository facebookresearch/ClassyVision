{
  "name": "classification_task",
  "num_epochs": 90,
  "loss": {
    "name": "CrossEntropyLoss"
  },
  "dataset": {
    "train": {
      "name": "image_path",
      "batchsize_per_replica": 32,
      "num_samples": null,
      "use_shuffle": true,
      "transforms": [
        {
          "name": "apply_transform_to_key",
          "transforms": [
            {"name": "RandomResizedCrop", "size": 224},
            {"name": "RandomHorizontalFlip"},
            {"name": "ToTensor"},
            {
              "name": "Normalize",
              "mean": [0.485, 0.456, 0.406],
              "std": [0.229, 0.224, 0.225]
            }
          ],
          "key": "input"
        }
      ],
      "image_folder": "/tmp/imagenet/train"
    },
    "test": {
      "name": "image_path",
      "batchsize_per_replica": 32,
      "num_samples": null,
      "use_shuffle": false,
      "transforms": [
        {
          "name": "apply_transform_to_key",
          "transforms": [
            {"name": "Resize", "size": 256},
            {"name": "CenterCrop", "size": 224},
            {"name": "ToTensor"},
            {
              "name": "Normalize",
              "mean": [0.485, 0.456, 0.406],
              "std": [0.229, 0.224, 0.225]
            }
          ],
          "key": "input"
        }
      ],
      "image_folder": "/tmp/imagenet/val"
    }
  },
  "meters": {
    "accuracy": {
      "topk": [1, 5]
    }
  },
  "model": {
    "name": "regnet",
    "depth": 17,
    "w_0": 192,
    "w_a": 76.82,
    "w_m": 2.19,
    "group_width": 56,
    "heads": [
      {
        "name": "fully_connected",
        "unique_id": "default_head",
        "num_classes": 1000,
        "in_plane": 2016,
        "fork_block": "trunk_output"
      }
    ]
  },
  "optimizer": {
    "name": "sgd",
    "param_schedulers": {
      "lr": {
        "name": "step",
        "values": [0.1, 0.01, 0.001]
      }
    },
    "weight_decay": 1e-4,
    "momentum": 0.9
  }
}
